{
  "name": "linkly-calendar",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm prisma:generate && pnpm infra:up && turbo run dev",
    "dev:with-infra": "pnpm prisma:generate && pnpm infra:up && turbo run dev",
    "prisma:generate": "prisma generate",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "lint": "turbo run lint",
    "init:env": "tsx scripts/init-local-env.ts"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "prisma": "^6.19.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
